<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default sync-j="4" revision="morty"/>

  <remote name="digi"  fetch="https://github.com/digi-embedded"/>
  <remote name="fsl"   fetch="https://github.com/Freescale"/>
  <remote name="qt5"   fetch="https://github.com/meta-qt5"/>
  <remote name="swu"   fetch="https://github.com/sbabic"/>
  <remote name="oe"    fetch="https://git.openembedded.org"/>
  <remote name="yocto" fetch="https://git.yoctoproject.org"/>

  <project name="meta-digi.git" path="sources/meta-digi" remote="digi" revision="morty">
    <copyfile src="sdk/mkproject.sh" dest="mkproject.sh"/>
  </project>
  <project name="meta-freescale.git"    path="sources/meta-freescale"    remote="yocto" revision="morty"/>
  <project name="meta-fsl-demos.git"    path="sources/meta-fsl-demos"    remote="fsl"   revision="morty"/>
  <project name="meta-openembedded.git" path="sources/meta-openembedded" remote="oe"    revision="morty"/>
  <project name="meta-qt5.git"          path="sources/meta-qt5"          remote="qt5"   revision="morty"/>
  <project name="meta-swupdate.git"     path="sources/meta-swupdate"     remote="swu"   revision="morty"/>
  <project name="poky.git"              path="sources/poky"              remote="yocto" revision="morty"/>
</manifest>
